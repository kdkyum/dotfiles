[core]
	editor = vim
	excludesfile = ~/.gitignore_global
  pager = less -F -X
[color]
	ui = true
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[alias]
	sla = log --oneline --decorate --graph --all
	car = commit --amend --no-edit
	uncommit = reset --soft HEAD^
	unstage = reset
